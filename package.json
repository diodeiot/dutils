{
  "name": "dutils",
  "displayName": "dutils",
  "description": "Utility commands",
  "version": "1.1.0",
  "publisher": "diodeiot",
  "engines": {
    "vscode": "^1.30.0"
  },
  "icon": "images/tools.png",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Kadir Sevil",
  "repository": {
    "type": "git",
    "url": "https://github.com/diodeiot/dutils"
  },
  "bugs": {
    "url": "https://github.com/diodeiot/dutils/issues"
  },
  "homepage": "https://github.com/diodeiot/dutils",
  "categories": [
    "Formatters",
    "Snippets"
  ],
  "keywords": [
    "utils",
    "C/C++",
    "include guard"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dutils.addIncludeGuard",
        "title": "dutils: Add include guard"
      },
      {
        "command": "dutils.createCFiles",
        "title": "dutils: Create C source & header files"
      },
      {
        "command": "dutils.createCppFiles",
        "title": "dutils: Create C++ source & header files"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.30.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "zod": "^3.22.4"
  }
}